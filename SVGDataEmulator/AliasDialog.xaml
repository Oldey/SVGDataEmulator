<Window x:Class="SVGDataEmulator.AliasWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SVGDataEmulator"
        mc:Ignorable="d"
        Title="Enter an alias" Height="171.227" Width="330" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="textBoxAlias" HorizontalAlignment="Left" Height="23" Margin="10,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="294"/>
        <TextBlock x:Name="labelAlias" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Please add a desired alias for &quot;" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="Cancel" HorizontalAlignment="Left" Margin="96,103,0,0" VerticalAlignment="Top" Width="75" Click="button_Copy_Click" RenderTransformOrigin="-0.213,0.25"/>

    </Grid>
</Window>
